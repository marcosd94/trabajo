<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{plantillaResolucionEvalHome.wire}"/>

   <param name="plantillaResolucionEvalFrom"/>
   <param name="plantillaResolucionEvalIdPlantillaResolucionEval" value="#{plantillaResolucionEvalHome.plantillaResolucionEvalIdPlantillaResolucionEval}"/>


   <navigation from-action="#{plantillaResolucionEvalHome.persist()}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/evaluacionDesempenho/plantillaResolucion/PlantillaResolucionList.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{plantillaResolucionEvalHome.update()}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/evaluacionDesempenho/plantillaResolucion/PlantillaResolucionList.xhtml"/>
      </rule>
   </navigation>
   
   	<restrict>#{identity.hasRole('plantillaResolucion_edit')}</restrict>

</page>

<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
 login-required="true">
 
 <begin-conversation join="true" flush-mode="MANUAL"/>
  
  <action execute="#{grupoPuestosController.initCabecera()}" on-postback="false"/>
  <action execute="#{admEnvioHomologacionFormController.init()}" on-postback="false"/>
  <param name="concursoPuestoAgrIdConcursoPuestoAgr" value="#{grupoPuestosController.idConcursoPuestoAgr}"/>

 <param name="from"/>
 <param name="usuAlta" value="#{cronogramaConcCabList.cronogramaConcCab.usuAlta}"/>
 <param name="usuMod" value="#{cronogramaConcCabList.cronogramaConcCab.usuMod}"/>
 
 <navigation from-action="#{admEnvioHomologacionFormController.nextTask()}">
      <rule if-outcome="nextTask">
         <end-conversation/>
         <redirect view-id="/seleccion/bandejaEntrada/BandejaEntradaList.xhtml"/>
      </rule>
 </navigation>
 
 <restrict>#{identity.hasRole('envioGruposHomologacion')}</restrict>
</page>

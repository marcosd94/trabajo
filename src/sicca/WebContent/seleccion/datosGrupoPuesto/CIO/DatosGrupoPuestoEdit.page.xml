          <param name="concursoPuestoAgrIdConcursoPuestoAgr" value="#{datosGrupoPuestoFormController.idConcursoPuestoAgr}"/>
         </redirect>
      </rule>
   </navigation>

 <restrict>#{identity.hasRole('datosGrupoPuesto_edit_cio')}</restrict>

</page>

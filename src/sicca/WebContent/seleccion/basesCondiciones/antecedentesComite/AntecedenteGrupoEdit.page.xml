<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	no-conversation-view-id="/AntecedenteGrupoList.xhtml" login-required="true">


	<begin-conversation join="true" flush-mode="MANUAL" />
	<param name="from"/>
 <param name="fromPantalla" value="#{antecedentesComiteeFormController.fromCU}"/>
  <param name="nomPantalla"/>
 <param name="idConcursoPuestoAgr" value="#{antecedentesComiteeFormController.idConcursoPuestoAgr}"/>
	
	<action execute="#{antecedenteGrupoHome.wire}" />
	<action execute="#{antecedentesComiteeFormController.init()}"
		on-postback="false" />
	

	<navigation from-action="#{antecedenteGrupoHome.persist}">
		<rule if-outcome="persisted">
			<end-conversation />
			<redirect view-id="/AntecedenteGrupo.xhtml" />
		</rule>
	</navigation>

	<navigation from-action="#{antecedenteGrupoHome.update}">
		<rule if-outcome="updated">
			<end-conversation />
			<redirect view-id="/AntecedenteGrupo.xhtml" />
		</rule>
	</navigation>

	<navigation from-action="#{antecedenteGrupoHome.remove}">
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/AntecedenteGrupoList.xhtml" />
		</rule>
	</navigation>

<restrict>#{identity.hasRole('bases_y_condiciones_antecedente')}</restrict>
</page>

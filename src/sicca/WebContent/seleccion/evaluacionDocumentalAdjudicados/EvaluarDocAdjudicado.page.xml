<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	login-required="true">
	<begin-conversation flush-mode="MANUAL" join="true" />
	<action execute="#{evaluacionDocAdjudicadosFormController.init()}"
		on-postback="false" />


	<param name="from" />
	<param name="CU544" value="#{evaluacionDocAdjudicadosFormController.volvio}"/>
	<param name="bloquearNoAprobados" value="#{evaluacionDocAdjudicadosFormController.bloquearNoAprobados}" />
	<param name="concursoPuestoAgrIdConcursoPuestoAgr"
		value="#{concursoPuestoAgrHome.concursoPuestoAgrIdConcursoPuestoAgr}" />

<navigation from-action="#{evaluacionDocAdjudicadosFormController.nextTask()}">
		<rule if-outcome="next">
			<end-conversation />
			<redirect view-id="/seleccion/bandejaEntrada/BandejaEntradaList.xhtml" />
		</rule>
	</navigation>

	<restrict>#{identity.hasRole('evaluacion_doc_adjudicados')}</restrict>
</page>

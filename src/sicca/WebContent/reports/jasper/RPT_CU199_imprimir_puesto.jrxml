	<subDataset name="valorEconomico">
		<parameter name="idPlantaCargoDet" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Long(23)]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select *
from planificacion.puesto_concepto_pago
where id_planta_cargo_det = $P{idPlantaCargoDet}
and activo is true
order by obj_codigo, categoria]]>
		</queryString>
		<field name="id_puesto_concepto_pago" class="java.lang.Long"/>
		<field name="id_planta_cargo_det" class="java.lang.Long"/>
		<field name="monto" class="java.lang.Integer"/>
		<field name="usu_alta" class="java.lang.String"/>
		<field name="usu_mod" class="java.lang.String"/>
		<field name="fecha_alta" class="java.sql.Timestamp"/>
		<field name="fecha_mod" class="java.sql.Timestamp"/>
		<field name="categoria" class="java.lang.String"/>
		<field name="obj_codigo" class="java.lang.Integer"/>
		<field name="anho" class="java.lang.Integer"/>
		<field name="disponible" class="java.lang.Boolean"/>
		<field name="activo" class="java.lang.Boolean"/>
	</subDataset>
	<subDataset name="nombramiento">
		<parameter name="idPlantaCargoDet" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Long(23)]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select distinct tp.descripcion as tipo_planta, tn.descripcion as tipo_nombramiento
from planificacion.det_tipo_nombramiento dtn

join planificacion.tipo_nombramiento tn
on (dtn.id_tipo_nombramiento = tn.id_tipo_nombramiento)

join planificacion.tipo_planta tp
on (tn.id_tipo_planta = tp.id_tipo_planta)

where dtn.id_planta_cargo_det = $P{idPlantaCargoDet}
and tn.activo is true
and tp.activo is true]]>
		</queryString>
		<field name="tipo_planta" class="java.lang.String"/>
		<field name="tipo_nombramiento" class="java.lang.String"/>
	</subDataset>
	<subDataset name="contenidoFuncional">
		<parameter name="idPlantaCargoDet" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Long(23)]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
    id_det_contenido_funcional,
    cf.descripcion as contenido_funcional,
    puntaje,
    ddcf.descripcion

FROM planificacion.det_contenido_funcional dcf

join planificacion.contenido_funcional cf
on(dcf.id_contenido_funcional = cf.id_contenido_funcional)

join planificacion.det_descripcion_cont_funcional ddcf
on(dcf.id_det_contenido_funcional = ddcf.id_contenido_funcional)

where cf.activo is true
    and ddcf.activo is true
    and id_planta_cargo_det = $P{idPlantaCargoDet}

order by cf.orden]]>
		</queryString>
		<field name="id_det_contenido_funcional" class="java.lang.Long"/>
		<field name="contenido_funcional" class="java.lang.String"/>
		<field name="puntaje" class="java.lang.Float"/>
		<field name="descripcion" class="java.lang.String"/>
	</subDataset>
	<subDataset name="req_minimos">
		<parameter name="idPlantaCargoDet" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Long(23)]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
    rmc.descripcion as componente,
    drm.puntaje_req_min,
    minimos_requeridos as minimos_requeridos,
    null as opciones_convenientes,
    rmc.orden

FROM planificacion.det_req_min drm

join planificacion.det_minimos_requeridos dmr
on (drm.id_det_req_min = dmr.id_det_req_min)

join planificacion.requisito_minimo_cpt rmc
on (drm.id_requisito_minimo_cpt = rmc.id_requisito_minimo_cpt)

where dmr.activo is true
    and rmc.activo is true
    --and drm.activo is true
    and id_planta_cargo_det = $P{idPlantaCargoDet}

UNION

SELECT
    rmc.descripcion as componente,
    drm.puntaje_req_min,
    null as minimos_requeridos,
    opc_convenientes as opciones_convenientes,
    rmc.orden

FROM planificacion.det_req_min drm

join planificacion.det_opciones_convenientes doc
on (drm.id_det_req_min = doc.id_det_req_min)

join planificacion.requisito_minimo_cpt rmc
on (drm.id_requisito_minimo_cpt = rmc.id_requisito_minimo_cpt)

where doc.activo is true
    and rmc.activo is true
    --and drm.activo is true
    and id_planta_cargo_det = $P{idPlantaCargoDet}

ORDER BY orden, minimos_requeridos, opciones_convenientes]]>
		</queryString>
		<field name="componente" class="java.lang.String"/>
		<field name="puntaje_req_min" class="java.lang.Float"/>
		<field name="minimos_requeridos" class="java.lang.String"/>
		<field name="opciones_convenientes" class="java.lang.String"/>
		<field name="orden" class="java.lang.Integer"/>
	</subDataset>

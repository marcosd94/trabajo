	<queryString>
		<![CDATA[SELECT
    pcd.id_planta_cargo_det,
    pcd.activo,
    sne.nen_codigo as nivel_codigo,
    sne.nen_nombre as nivel,
    se.ent_codigo as entidad_codigo,
    se.ent_nombre as entidad,
    cuc.orden as oee_codigo,
    cuc.denominacion_unidad as oee,
    cud.denominacion_unidad as unidad_org,
    tcpt.descripcion as tipo_cpt,
    cpt.denominacion as cpt,
    pcd.descripcion, pcd.orden,
    pcd.contratado, pcd.permanente,
    pcd.fundamentacion_tecnica,
    pcd.mision, pcd.objetivo,
    esp.descripcion as especialidad,
    lu_hora_desde,
    ma_hora_desde,
    mi_hora_desde,
    ju_hora_desde,
    vi_hora_desde,
    sa_hora_desde,
    do_hora_desde,
    lu_hora_hasta,
    ma_hora_hasta,
    mi_hora_hasta,
    ju_hora_hasta,
    vi_hora_hasta,
    sa_hora_hasta,
    do_hora_hasta,
    o.descripcion as oficina,
    p.nombres, p.apellidos, p.documento_identidad,
    ec.descripcion as estado


FROM planificacion.planta_cargo_det pcd



left join planificacion.configuracion_uo_det cud
on (pcd.id_configuracion_uo_det = cud.id_configuracion_uo_det)

left join planificacion.configuracion_uo_cab cuc
on (cuc.id_configuracion_uo = cud.id_configuracion_uo)

left join planificacion.entidad e
on (e.id_entidad = cuc.id_entidad)

left join sinarh.sin_entidad se
on (e.id_sin_entidad = se.id_sin_entidad)

left join sinarh.sin_nivel_entidad sne
on (sne.ani_aniopre = se.ani_aniopre and sne.nen_codigo = se.nen_codigo)


left join planificacion.cpt cpt
on (cpt.id_cpt = pcd.id_cpt)

left join planificacion.tipo_cpt tcpt
on (cpt.id_tipo_cpt = tcpt.id_tipo_cpt)

left join planificacion.especialidades esp
on (pcd.id_especialidades = esp.id_especialidades)

LEFT join planificacion.oficina o
on (pcd.id_oficina = o.id_oficina)

LEFT join general.empleado_puesto ep
on (pcd.id_planta_cargo_det = ep.id_planta_cargo_det and ep.actual is true)

LEFT join general.persona p
on (ep.id_persona = p.id_persona)

LEFT join planificacion.estado_cab ec
on (pcd.id_estado_cab = ec.id_estado_cab)

where pcd.id_planta_cargo_det = $P{idPlantaCargoDet}]]>
	</queryString>

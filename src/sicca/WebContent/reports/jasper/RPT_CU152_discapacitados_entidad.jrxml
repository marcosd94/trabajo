	<queryString>
		<![CDATA[SELECT distinct
oee.orden as orden_oee,
oee.id_configuracion_uo as id_oee,
oee.denominacion_unidad oee,
uo.orden as orden_uo,
uo.id_configuracion_uo_det as id_uo,
uo.denominacion_unidad uo,
puesto.orden as orden_puesto,
puesto.id_planta_cargo_det as id_puesto,
puesto.descripcion puesto,
persona.nombres,
persona.apellidos,
persona.documento_identidad,
(   SELECT  array_to_string(array_agg(espec.descripcion ), ', ')
    FROM seleccion.datos_especificos espec
    INNER JOIN seleccion.datos_generales grales
    ON espec.id_datos_generales = grales.id_datos_generales

    join seleccion.persona_discapacidad discapacidad
    on discapacidad.id_persona = persona.id_persona

    WHERE  grales.descripcion LIKE 'DISCAPACIDADES'
    and discapacidad.id_datos_especificos_discapac = espec.id_datos_especificos
) discapacidades

FROM planificacion.configuracion_uo_cab oee

join planificacion.configuracion_uo_det uo
on oee.id_configuracion_uo = uo.id_configuracion_uo_det

join planificacion.planta_cargo_det puesto
on puesto.id_configuracion_uo_det = uo.id_configuracion_uo_det

join general.empleado_puesto emp
on emp.id_planta_cargo_det = puesto.id_planta_cargo_det

join general.persona
on emp.id_persona = persona.id_persona

join seleccion.persona_discapacidad discapacidad
on discapacidad.id_persona = persona.id_persona

join planificacion.entidad
on oee.id_entidad = entidad.id_entidad

join sinarh.sin_entidad
on entidad.id_sin_entidad = sin_entidad.id_sin_entidad

WHERE sin_entidad.id_sin_entidad = $P{idSinEntidad}
AND (oee.id_configuracion_uo = $P{idOEE} or $P{idOEE} is null)
AND (uo.id_configuracion_uo_det = $P{idUO} or $P{idUO} is null)
AND UPPER(persona.nombres) LIKE $P{nombre}
AND UPPER(persona.apellidos) LIKE $P{apellido}
AND UPPER(persona.documento_identidad) LIKE $P{documento}

order by oee.orden, id_oee, uo.orden, id_uo, puesto.orden, id_puesto, persona.nombres, persona.apellidos]]>
	</queryString>

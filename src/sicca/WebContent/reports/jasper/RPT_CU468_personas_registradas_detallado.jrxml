	</parameter>
	<parameter name="idTitulo" class="java.lang.Long" isForPrompting="false">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="idEspecialidad" class="java.lang.Long" isForPrompting="false">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="desde" class="java.util.Date" isForPrompting="false">
		<defaultValueExpression><![CDATA[new Date("01/01/2009")]]></defaultValueExpression>
	</parameter>
	<parameter name="hasta" class="java.util.Date" isForPrompting="false">
		<defaultValueExpression><![CDATA[new Date("01/01/2015")]]></defaultValueExpression>
	</parameter>
	<parameter name="titulo" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[null]]></defaultValueExpression>
	</parameter>
	<parameter name="especialidad" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[null]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	USU.CODIGO_USUARIO,
          PER.id_persona,
	PER.NOMBRES ||' ' || PER.APELLIDOS as nombre,
	CASE WHEN PER.ES_FUNCIONARIO = TRUE THEN 'SI' WHEN PER.ES_FUNCIONARIO = FALSE
	THEN 'NO' ELSE 'SIN DEFINIR' END as es_funcionario,
	PER.CALLE_PRINCIPAL,
	PER.documento_identidad,
	PER.TELEFONOS,
	PER.E_MAIL,
	TIT.DESCRIPCION as titulo,
	ES.OTRO_TITULO_OBT,
	INS.DESCRIPCION as institucion,
	ESP.DESCRIPCION as especialidad,
	(SELECT count(ID_PERSONA) FROM SELECCION.FUNCIONARIO_OEE WHERE ID_PERSONA = PER.id_persona) as cantidadFuncionarios,
	(SELECT count(ID_PERSONA) FROM SELECCION.PERSONA_DISCAPACIDAD WHERE ID_PERSONA = PER.id_persona) as cantidadDiscapacidad

FROM GENERAL.PERSONA PER

JOIN SEGURIDAD.USUARIO USU
ON PER.ID_PERSONA = USU.ID_PERSONA

LEFT JOIN SELECCION.ESTUDIOS_REALIZADOS ES
ON ES.ID_PERSONA = PER.ID_PERSONA

LEFT JOIN SELECCION.TITULO_ACADEMICO TIT
ON TIT.ID_TITULO_ACADEMICO = ES.ID_TITULO_ACADEMICO

LEFT JOIN SELECCION.INSTITUCION_EDUCATIVA INS
ON INS.ID_INST_EDUCATIVA = ES.ID_INST_EDUCATIVA

LEFT JOIN PLANIFICACION.ESPECIALIDADES ESP
ON ESP.ID_ESPECIALIDADES = ES.ID_ESPECIALIDAD

WHERE USU.USU_ALTA  = 'PORTAL'
AND cast(USU.FECHA_ALTA as date) BETWEEN cast($P{desde} as date) AND cast($P{hasta} as date)
AND (TIT.ID_TITULO_ACADEMICO = $P{idTitulo} or $P{idTitulo} is null)
AND (ESP.ID_ESPECIALIDADES = $P{idEspecialidad} or $P{idEspecialidad} is null)
ORDER BY 1]]>
	</queryString>

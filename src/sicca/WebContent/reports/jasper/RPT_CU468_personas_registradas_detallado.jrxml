	<subDataset name="InstitucionesDS">
		<parameter name="idPersona" class="java.lang.Long" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
	OEE.DENOMINACION_UNIDAD,
	CASE WHEN FUN.MODALIDADOCUPACION = 'C' THEN 'CONTRATATO'
	WHEN FUN.MODALIDADOCUPACION = 'P' THEN 'PERMANENTE' END as modalidad,
	TIPO.DESCRIPCION as tipo_nombramiento,
	FUN.FECHA_INGRESO

FROM SELECCION.FUNCIONARIO_OEE FUN

LEFT JOIN PLANIFICACION.TIPO_NOMBRAMIENTO TIPO
ON TIPO.ID_TIPO_NOMBRAMIENTO = FUN.ID_TIPO_NOMBRAMIENTO

JOIN PLANIFICACION.CONFIGURACION_UO_CAB OEE
ON OEE.ID_CONFIGURACION_UO = FUN.ID_CONFIGURACION_UO

WHERE FUN.ID_PERSONA = $P{idPersona}]]>
		</queryString>
		<field name="denominacion_unidad" class="java.lang.String"/>
		<field name="modalidad" class="java.lang.String"/>
		<field name="tipo_nombramiento" class="java.lang.String"/>
		<field name="fecha_ingreso" class="java.sql.Date"/>
	</subDataset>
	<subDataset name="DiscapacidadDS">
		<parameter name="idPersona" class="java.lang.Long" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT  DE.DESCRIPCION as discapacidad,
	AUT.DESCRIPCION as autonomia,
	DIS.DIFICULTAD_ACTIVIDAD

FROM SELECCION.PERSONA_DISCAPACIDAD DIS

JOIN SELECCION.DATOS_ESPECIFICOS DE
ON DE.ID_DATOS_ESPECIFICOS = DIS.ID_DATOS_ESPECIFICOS_DISCAPAC

LEFT JOIN SELECCION.DATOS_ESPECIFICOS AUT
ON AUT.ID_DATOS_ESPECIFICOS = DIS.ID_DATOS_ESPECIFICOS_GRADO_AUTONOM

WHERE DIS.ID_PERSONA = $P{idPersona}]]>
		</queryString>
		<field name="discapacidad" class="java.lang.String"/>
		<field name="autonomia" class="java.lang.String"/>
		<field name="dificultad_actividad" class="java.lang.String"/>
	</subDataset>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["C:\\work\\sicca\\03.implementacion\\sistema integrado\\sicca\\WebContent\\reports\\jasper"]]></defaultValueExpression>
	</parameter>

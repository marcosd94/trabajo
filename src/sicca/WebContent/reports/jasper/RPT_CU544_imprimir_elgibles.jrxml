	<queryString>
		<![CDATA[SELECT
  persona_post.nombres,
  persona_post.apellidos,
  eval.puntaje_realizado,
  seleccion.lista_elegibles_det.obs_estado
FROM
  seleccion.eval_referencial_postulante eval
  INNER JOIN seleccion.postulacion post ON (post.id_postulacion = eval.id_postulacion)
  INNER JOIN seleccion.persona_postulante persona_post ON (post.id_persona_postulante = persona_post.id_persona_postulante)
  INNER JOIN seleccion.lista_elegibles_det ON (post.id_postulacion = seleccion.lista_elegibles_det.id_postulacion)
  INNER JOIN seleccion.lista_elegibles_cab ON (seleccion.lista_elegibles_cab.id_lista_elegibles_cab = seleccion.lista_elegibles_det.id_lista_elegibles_cab)
WHERE
  post.activo = TRUE AND
  eval.activo = TRUE
AND eval.ID_CONCURSO_PUESTO_AGR = $P{idGrupo}
ORDER BY
    eval.puntaje_realizado DESC]]>
	</queryString>

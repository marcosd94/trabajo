	<queryString>
		<![CDATA[SELECT sne.nen_codigo as nivel_codigo, sne.nen_nombre as nivel_nombre, sne.id_sin_nivel_entidad,
    se.ent_codigo as entidad_codigo, se.ent_nombre as entidad_nombre, se.id_sin_entidad,
    cuc.orden as oee_codigo, cuc.denominacion_unidad as oee_nombre, cuc.id_configuracion_uo,
    meses.nombre as mes,
    "planificacion"."cant_empleados_anho" ($P{anho}, meses.id_mes, cuc.id_configuracion_uo, true)  as total_c,
    "planificacion"."cant_empleados_anho" ($P{anho}, meses.id_mes, cuc.id_configuracion_uo, false) as total_p

FROM planificacion.configuracion_uo_cab cuc

JOIN planificacion.entidad e
on(cuc.id_entidad = e.id_entidad)

JOIN sinarh.sin_entidad se
on(se.id_sin_entidad= e.id_sin_entidad)

JOIN sinarh.sin_nivel_entidad sne
on (se.ani_aniopre = sne.ani_aniopre and se.nen_codigo = sne.nen_codigo)

JOIN(
select 1 as id_mes, 'ENE' as nombre
UNION select 2 as id_mes, 'FEB' as nombre
UNION select 3 as id_mes, 'MAR' as nombre
UNION select 4 as id_mes, 'ABR' as nombre
UNION select 5 as id_mes, 'MAY' as nombre
UNION select 6 as id_mes, 'JUN' as nombre
UNION select 7 as id_mes, 'JUL' as nombre
UNION select 8 as id_mes, 'AGO' as nombre
UNION select 9 as id_mes, 'SET' as nombre
UNION select 10 as id_mes, 'OCT' as nombre
UNION select 11 as id_mes, 'NOV' as nombre
UNION select 12 as id_mes, 'DIC' as nombre
) meses on true

where cuc.activo is true
and se.activo is true
and sne.activo is true
and sne.id_sin_nivel_entidad = $P{idSinNivelEntidad}
and se.id_sin_entidad = $P{idSinEntidad}
and cuc.id_configuracion_uo = $P{idOEE}

order by meses.id_mes]]>
	</queryString>

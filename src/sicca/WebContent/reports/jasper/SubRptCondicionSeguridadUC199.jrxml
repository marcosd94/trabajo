	<queryString>
		<![CDATA[SELECT cs.descripcion AS condicion_seguridad,
    dcs.puntaje_cond_segur AS puntaje,
    dcs.justificacion AS justificacion,
    dcs.ajustes AS ajustes
FROM planificacion.det_condicion_segur dcs
    INNER JOIN planificacion.condicion_segur cs
        ON dcs.id_condicion_segur = cs.id_condicion_segur
    INNER JOIN planificacion.planta_cargo_det pcd
        ON dcs.id_planta_cargo_det = pcd.id_planta_cargo_det
WHERE pcd.id_planta_cargo_det = $P{idPlantaCargoDet}
    AND lower(dcs.tipo) = 'puesto'
    AND dcs.activo = true]]>
	</queryString>

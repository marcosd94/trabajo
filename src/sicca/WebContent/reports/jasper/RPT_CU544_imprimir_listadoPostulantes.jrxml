	<queryString>
		<![CDATA[SELECT
    p.observacion,
    c.descripcion   as puesto
FROM
    seleccion.concurso_puesto_det p,
    planificacion.estado_det ed,
    planificacion.planta_cargo_det c,
    planificacion.estado_cab ec
WHERE
   p.id_concurso_puesto_agr =  $P{idGrupo}
and p.id_estado_det = ed.id_estado_det
AND ec.descripcion LIKE '%CONCURSO%'
AND ec.id_estado_cab = ed.id_estado_cab
AND p.id_planta_cargo_det = c.id_planta_cargo_det
order by c.descripcion]]>
	</queryString>

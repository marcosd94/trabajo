	<subDataset name="accion">
		<parameter name="id" class="java.lang.Long" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT CASE WHEN AC.ESTADO = 'P' THEN 'PENDIENTE CON MEDIDA CAUTELAR' WHEN AC.ESTADO = 'A' THEN 'ACUERDO Y SENTENCIA' END AS ACCION,
AC.FECHA_ALTA AS FECHA,
AC.RESULTADO AS RESULTADO
FROM JURIDICOS.ACCION_INCONST_CAB AC
where AC.ID_PERSONA = $P{id}
ORDER BY AC.FECHA_ALTA DESC]]>
		</queryString>
		<field name="accion" class="java.lang.String"/>
		<field name="fecha" class="java.sql.Timestamp"/>
		<field name="resultado" class="java.lang.String"/>
	</subDataset>
	<subDataset name="sumario">
		<parameter name="id" class="java.lang.Long" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT SC.FECHA_ALTA AS FECHA_SUM,OEE.DENOMINACION_UNIDAD AS OEE,
CASE WHEN SC.ESTADO = 'EC' THEN 'EN CURSO'
		WHEN SC.ESTADO = 'RJ' THEN 'CON RESOLUCION DEL JUEZ'
		WHEN SC.ESTADO = 'SO' THEN 'SOBRESEIMIENTO'
		WHEN SC.ESTADO = 'SA' THEN 'SANCION'
	WHEN SC.ESTADO = 'AR' THEN 'ARCHIVO' END AS ESTADO_SUMARIO,
	SC.NRO_EXP || '/' ||SC.ANIO_EXP AS Nro_Interno,
	SC.NRO_EXP_SFP || '/' ||SC.ANIO_EXP_SFP AS Nro_sfp,
		CASE WHEN SC.ESTADO_JUEZ = 'SO' THEN 'SOBRESEIMIENTO'
WHEN SC.ESTADO_JUEZ = 'SA' THEN 'SANCION' ||' - '|| S.DESCRIPCION ||' - '|| SC.TIEMPO_J ||' '|| CASE WHEN SC.EXPRESADO_J = 'D' THEN 'DÍA/S' WHEN SC.EXPRESADO_J = 'M' THEN 'MES/ES' WHEN SC.EXPRESADO_J = 'A' THEN 'AÑO/S' END
WHEN SC.ESTADO_JUEZ = 'AR' THEN 'ARCHIVO' END ||' - Obs.: '|| SC.OBS_J AS OBS_JUEZ,
	CASE WHEN SC.ESTADO = 'SO' THEN 'SOBRESEIMIENTO - MAI'
WHEN SC.ESTADO = 'SA' THEN 'SANCION MAI - ' || SD.DESCRIPCION ||' '|| CASE WHEN SD.INHABILITA = TRUE THEN SC.TIEMPO_M ||' '|| CASE WHEN SC.EXPRESADO_M = 'D' THEN 'DÍA/S' WHEN SC.EXPRESADO_M = 'M' THEN 'MES/ES' WHEN SC.EXPRESADO_M = 'A' THEN 'AÑO/S' END ELSE  SD.DESCRIPCION END
	WHEN SC.ESTADO = 'AR' THEN 'ARCHIVO - MAI' END ||' - Obs.: '|| SC.OBS_M AS OBS_MAI,
	--DE.DESCRIPCION AS FALTAS,
	SC.ID_SUMARIO_CAB AS ID_CAB--, SDET.ID_SUMARIO_DET AS ID_DET
FROM JURIDICOS.SUMARIO_CAB SC
LEFT JOIN JURIDICOS.SANCION S ON S.ID_SANCION = SC.ID_SANCION_J
LEFT JOIN JURIDICOS.SANCION SD ON SD.ID_SANCION = SC.ID_SANCION_M
JOIN PLANIFICACION.CONFIGURACION_UO_CAB OEE ON OEE.ID_CONFIGURACION_UO = SC.ID_CONFIGURACION_UO
--JOIN JURIDICOS.SUMARIO_DET SDET ON SDET.ID_SUMARIO_CAB = SC.ID_SUMARIO_CAB
--JOIN SELECCION.DATOS_ESPECIFICOS DE ON DE.ID_DATOS_ESPECIFICOS= SDET.ID_DATOS_ESPECIF_FALTA
WHERE SC.ID_PERSONA = $P{id}
ORDER BY SC.FECHA_ALTA]]>
		</queryString>
		<field name="fecha_sum" class="java.sql.Timestamp"/>
		<field name="oee" class="java.lang.String"/>
		<field name="estado_sumario" class="java.lang.String"/>
		<field name="nro_interno" class="java.lang.String"/>
		<field name="nro_sfp" class="java.lang.String"/>
		<field name="obs_juez" class="java.lang.String"/>
		<field name="obs_mai" class="java.lang.String"/>
		<field name="id_cab" class="java.lang.Long"/>
	</subDataset>
	<subDataset name="inhabilitado">
		<parameter name="id" class="java.lang.Long" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT CASE WHEN INH.INHABILITADO = TRUE THEN 'NO' ELSE 'SI' END as inhabilitado,
INH.FECHA_DESDE, INH.FECHA_HASTA, INH.FECHA_HABILIT as fecha_habilitacion,
CASE WHEN INH.TIPO = 'D' THEN 'DESVINCULACION' WHEN INH.TIPO = 'J' THEN 'PROCESOS JURIDICOS' END as motivo
FROM DESVINCULACION.INHABILITADOS INH
WHERE INH.ID_PERSONA = $P{id}]]>
		</queryString>
		<field name="inhabilitado" class="java.lang.String"/>
		<field name="fecha_desde" class="java.sql.Timestamp"/>
		<field name="fecha_hasta" class="java.sql.Timestamp"/>
		<field name="fecha_habilitacion" class="java.sql.Timestamp"/>
		<field name="motivo" class="java.lang.String"/>
	</subDataset>
	<subDataset name="leyes">
		<parameter name="id" class="java.lang.Long" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
CASE WHEN DE.DESCRIPCION IS NULL THEN DET.OTRA_LEY ELSE DE.DESCRIPCION END AS LEY,
CASE WHEN DET.ART_TODOS = TRUE THEN 'TODOS' ELSE 'SOLO LOS ART.: ' || DET.ART_ESPECIF END AS ART
FROM JURIDICOS.ACCION_INCONST_CAB AC
JOIN JURIDICOS.ACCION_INCONST_DET DET ON DET.ID_ACCION_CAB = AC.ID_ACCION_CAB
JOIN SELECCION.DATOS_ESPECIFICOS DE ON DE.ID_DATOS_ESPECIFICOS = DET.ID_DATOS_ESPECIF_LEY
WHERE AC.ID_PERSONA = $P{id}
ORDER BY AC.FECHA_ALTA DESC]]>
		</queryString>
		<field name="ley" class="java.lang.String"/>
		<field name="art" class="java.lang.String"/>
	</subDataset>
	<subDataset name="faltas">
		<parameter name="id" class="java.lang.Long" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT DE.DESCRIPCION AS FALTAS,  SDET.ID_SUMARIO_DET AS ID_DET
FROM JURIDICOS.SUMARIO_CAB SC
LEFT JOIN JURIDICOS.SANCION S ON S.ID_SANCION = SC.ID_SANCION_J
LEFT JOIN JURIDICOS.SANCION SD ON SD.ID_SANCION = SC.ID_SANCION_M
JOIN PLANIFICACION.CONFIGURACION_UO_CAB OEE ON OEE.ID_CONFIGURACION_UO = SC.ID_CONFIGURACION_UO
JOIN JURIDICOS.SUMARIO_DET SDET ON SDET.ID_SUMARIO_CAB = SC.ID_SUMARIO_CAB
JOIN SELECCION.DATOS_ESPECIFICOS DE ON DE.ID_DATOS_ESPECIFICOS= SDET.ID_DATOS_ESPECIF_FALTA
WHERE SC.ID_PERSONA = $P{id}
ORDER BY SC.FECHA_ALTA]]>
		</queryString>
		<field name="faltas" class="java.lang.String"/>
	</subDataset>

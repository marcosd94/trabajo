	<queryString>
		<![CDATA[SELECT
    COUNT(*),persona.sexo,
    CASE
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 0 AND 15
        THEN '0-15'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 16 AND 20
        THEN '16-20'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 21 AND 25
        THEN '21-25'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 26 AND 30
        THEN '26-30'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 31 AND 35
        THEN '31-35'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 36 AND 40
        THEN '36-40'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 41 AND 45
        THEN '41-45'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 46 AND 50
        THEN '46-50'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 51 AND 55
        THEN '51-55'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 56 AND 60
        THEN '56-60'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT BETWEEN 61 AND 65
        THEN '61-65'
        WHEN substring(age(now(),persona.fecha_nacimiento)::text
        FROM
            1 FOR 2)::INT >= 66
        THEN '66 o mas'
    END AS grupo_etario
FROM
    sinarh.sin_nivel_entidad nivel,
    sinarh.sin_entidad sin_entidad
INNER JOIN planificacion.entidad entidad
ON
    entidad.id_sin_entidad = sin_entidad.id_sin_entidad
INNER JOIN planificacion.configuracion_uo_cab oee
ON
    oee.id_entidad = entidad.id_entidad
INNER JOIN planificacion.configuracion_uo_det uo
ON
    uo.id_configuracion_uo = oee.id_configuracion_uo
INNER JOIN planificacion.planta_cargo_det puesto
ON
    puesto.id_configuracion_uo_det = uo.id_configuracion_uo_det
INNER JOIN general.empleado_puesto emp
ON
    emp.id_planta_cargo_det = puesto.id_planta_cargo_det
INNER JOIN general.persona persona
ON
    emp.id_persona = persona.id_persona
WHERE
    emp.actual = 'true'
AND sin_entidad.nen_codigo = nivel.nen_codigo
AND sin_entidad.ani_aniopre = nivel.ani_aniopre
AND entidad.id_sin_entidad = $P{idSinEntidad}
AND oee.id_configuracion_uo = $P{idOEE}
GROUP BY
    grupo_etario, persona.sexo
ORDER BY
    grupo_etario]]>
	</queryString>

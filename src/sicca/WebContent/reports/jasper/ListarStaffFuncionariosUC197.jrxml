	<queryString>
		<![CDATA[SELECT p.documento_identidad AS cedula,
    p.nombres || ' ' || p.apellidos AS funcionario,
    p.calle_principal AS direccion,
    p.telefonos AS telefonos,
    p.e_mail AS email,
    case(ep.activo)
    when true then 'SI'
    else 'NO' end AS activo
FROM seleccion.empresa_persona ep INNER JOIN seleccion.empr_tercerizada emp
ON ep.id_empresa_tercerizada = emp.id_empresa_tercerizada INNER JOIN general.persona p
ON ep.id_persona = p.id_persona
$P!{whereClause}
ORDER BY funcionario]]>
	</queryString>

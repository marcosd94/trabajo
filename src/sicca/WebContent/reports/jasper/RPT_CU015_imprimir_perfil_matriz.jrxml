	<queryString>
		<![CDATA[SELECT
   agr.id_concurso_puesto_agr, descripcion_grupo, agr.estado, agr.mision, agr.objetivo, modalidad,
   fuente_financiacion, remuneracion, horario, cond_laborales, otras_inf,
   post_habilitados, post_no_habilitados, documentos, lugar, mision_especifica,
   fgp.fecha_publicacion_hasta as fecha_limite, fgp.hora_publicacion_hasta as hora_limite,
   contacto, otros_beneficios, otras_remuneraciones, id_planta_cargo_det,
   d_esp.descripcion as tipo_concurso,
    case when carpeta is true then 'SI'
       else 'NO' end as rec_carpeta,
    case when pcd is true then E'\nPara Personas con Discapacidad'
       else '' end as concursoPCD,
    case when ad_referendum is true then E'\nAd Referendum'
       else '' end as ad_referendum,
   (SELECT denominacion_unidad FROM planificacion.configuracion_uo_cab WHERE id_configuracion_uo =
   (SELECT id_configuracion_uo FROM seleccion.concurso WHERE id_concurso =
   (SELECT id_concurso from seleccion.concurso_puesto_agr where id_concurso_puesto_agr=$P{idConcursoPuestoAgr} ))) as institucion,
   (SELECT nombre FROM seleccion.concurso WHERE id_concurso =
   (SELECT id_concurso from seleccion.concurso_puesto_agr where id_concurso_puesto_agr=$P{idConcursoPuestoAgr} )) as nombre_concurso,
   (SELECT COUNT(*)FROM seleccion.concurso_puesto_det det WHERE id_concurso_puesto_agr=$P{idConcursoPuestoAgr}) as cantPuesto,
   "seleccion"."get_ubicacion_puesto_grupo" (agr.id_concurso_puesto_agr) as ubicaciones,
   "seleccion"."get_oficinas_puesto_grupo" (agr.id_concurso_puesto_agr) as oficinas,
   "seleccion"."get_datos_especificos"('PROCESO DE EVALUACION') as proceso_evaluacion,
   "seleccion"."get_datos_especificos"('PRORROGA DEL LLAMADO') as prorroga_llamado,
   "seleccion"."get_datos_especificos"('CONFIDENCIALIDAD') as confidencialidad,
    pgd.descripcion as descripcion_puesto

FROM seleccion.concurso_puesto_agr agr join planificacion.planta_cargo_det pgd on
pgd.id_planta_cargo_det = (SELECT id_planta_cargo_det FROM seleccion.concurso_puesto_det WHERE id_concurso_puesto_agr = $P{idConcursoPuestoAgr} LIMIT 1)
join seleccion.concurso con on con.id_concurso = agr.id_concurso
join seleccion.datos_especificos d_esp on con.id_datos_esp_tipo_conc = d_esp.id_datos_especificos
left join seleccion.fechas_grupo_puesto fgp on fgp.id_concurso_puesto_agr = agr.id_concurso_puesto_agr

where agr.activo = true
and agr.id_concurso_puesto_agr = $P{idConcursoPuestoAgr}]]>
	</queryString>

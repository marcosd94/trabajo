	<subDataset name="contenidoFuncional">
		<parameter name="idCpt" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
    id_det_contenido_funcional,
    cf.descripcion as contenido_funcional,
    puntaje,
    ddcf.descripcion

FROM planificacion.det_contenido_funcional dcf

join planificacion.contenido_funcional cf
on(dcf.id_contenido_funcional = cf.id_contenido_funcional)

join planificacion.det_descripcion_cont_funcional ddcf
on(dcf.id_det_contenido_funcional = ddcf.id_contenido_funcional)

where cf.activo is true
    and ddcf.activo is true
    and id_cpt = $P{idCpt}

order by cf.orden]]>
		</queryString>
		<field name="id_det_contenido_funcional" class="java.lang.Long"/>
		<field name="contenido_funcional" class="java.lang.String"/>
		<field name="puntaje" class="java.lang.Float"/>
		<field name="descripcion" class="java.lang.String"/>
	</subDataset>
	<subDataset name="req_minimos">
		<parameter name="idCpt" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Long(23)]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select distinct
req_min_cpt.orden,
req_min_cpt.descripcion as componente,
array_to_string(ARRAY(
	select

	det_min.minimos_requeridos as minimos_requeridos

	from planificacion.det_minimos_requeridos det_min
	join planificacion.det_req_min det_req_min
	on det_req_min.id_det_req_min = det_min.id_det_req_min

	join planificacion.requisito_minimo_cpt req_min_cpt_aux
	on req_min_cpt.id_requisito_minimo_cpt = det_req_min.id_requisito_minimo_cpt

	where det_min.id_det_req_min in (select id_det_req_min from planificacion.det_req_min drm where id_cpt = $P{idCpt})
	and req_min_cpt_aux.id_requisito_minimo_cpt = det_req_min.id_requisito_minimo_cpt



), '\n') as minimos_requeridos,

array_to_string(ARRAY(
	select

	det_opc.opc_convenientes as opciones_convenientes

	from planificacion.det_minimos_requeridos det_min
	join planificacion.det_req_min det_req_min
	on det_req_min.id_det_req_min = det_min.id_det_req_min

	join planificacion.requisito_minimo_cpt req_min_cpt_aux
	on req_min_cpt.id_requisito_minimo_cpt = det_req_min.id_requisito_minimo_cpt

	full join planificacion.det_opciones_convenientes det_opc
	on det_opc.id_det_req_min = det_min.id_det_req_min

	where det_min.id_det_req_min in (select id_det_req_min from planificacion.det_req_min drm where id_cpt = $P{idCpt})
	and req_min_cpt_aux.id_requisito_minimo_cpt = det_req_min.id_requisito_minimo_cpt



), '\n') as opciones_convenientes


from planificacion.det_minimos_requeridos det_min
join planificacion.det_req_min det_req_min
on det_req_min.id_det_req_min = det_min.id_det_req_min

join planificacion.requisito_minimo_cpt req_min_cpt
on req_min_cpt.id_requisito_minimo_cpt = det_req_min.id_requisito_minimo_cpt

where det_min.id_det_req_min in (select id_det_req_min from planificacion.det_req_min drm where id_cpt = $P{idCpt})

ORDER BY  orden,componente]]>
		</queryString>
		<field name="orden" class="java.lang.Integer"/>
		<field name="componente" class="java.lang.String"/>
		<field name="minimos_requeridos" class="java.lang.String"/>
		<field name="opciones_convenientes" class="java.lang.String"/>
	</subDataset>
	<subDataset name="req_minimos_escala">
		<parameter name="idCpt" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Long(23)]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select distinct

req_min_cpt.orden,

req_min_cpt.descripcion as componente,

array_to_string(ARRAY(

		select
			escalas_aux.escalas
		from
			planificacion.escala_req_min escala


		join (
			select distinct
			req_min_cpt.orden,
			req_min_cpt.descripcion as componente,
			escala.id_escala_req_min,
			escala.desde,
			escala.descripcion ||'\n\tEscala: ' || escala.desde:: text || '\t - ' || escala.hasta :: text ||'\n' as escalas

			from planificacion.det_minimos_requeridos det_min_aux
			join planificacion.det_req_min det_req_min
			on det_req_min.id_det_req_min = det_min_aux.id_det_req_min

			join planificacion.requisito_minimo_cpt req_min_cpt
			on req_min_cpt.id_requisito_minimo_cpt = det_req_min.id_requisito_minimo_cpt

			join planificacion.escala_req_min escala
			on escala.id_requisito_minimo_cpt = req_min_cpt.id_requisito_minimo_cpt

			where det_min_aux.id_det_req_min = det_min.id_det_req_min

			ORDER BY  orden,componente,desde
			) as escalas_aux

		on escala.id_escala_req_min = escalas_aux.id_escala_req_min


	), '\n') as escalas,
det_req_min.puntaje_req_min as puntaje

from planificacion.det_minimos_requeridos det_min
join planificacion.det_req_min det_req_min
on det_req_min.id_det_req_min = det_min.id_det_req_min

join planificacion.requisito_minimo_cpt req_min_cpt
on req_min_cpt.id_requisito_minimo_cpt = det_req_min.id_requisito_minimo_cpt

join planificacion.escala_req_min escala
on escala.id_requisito_minimo_cpt = req_min_cpt.id_requisito_minimo_cpt

where det_min.id_det_req_min in (select id_det_req_min from planificacion.det_req_min drm where id_cpt = $P{idCpt})

ORDER BY  orden,componente]]>
		</queryString>
		<field name="orden" class="java.lang.Integer"/>
		<field name="componente" class="java.lang.String"/>
		<field name="escalas" class="java.lang.String"/>
		<field name="puntaje" class="java.lang.Float"/>
	</subDataset>

	<subDataset name="PrincipalesTareasPuesto">
		<parameter name="idConcursoPuestoAgr" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Long(15)]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT distinct
    cf.orden  as row_number,
    cf.descripcion as columa1,
    ddcf.descripcion as columa2

FROM planificacion.det_descripcion_cont_funcional ddcf

join planificacion.det_contenido_funcional dcf
on (dcf.id_det_contenido_funcional = ddcf.id_contenido_funcional)

join planificacion.contenido_funcional cf
on (dcf.id_contenido_funcional = cf.id_contenido_funcional)

where cf.activo = true
and ddcf.activo = true
and dcf.tipo = 'GRUPO'
and id_concurso_puesto_agr = $P{idConcursoPuestoAgr}
order by row_number]]>
		</queryString>
		<field name="row_number" class="java.lang.Integer"/>
		<field name="columa1" class="java.lang.String"/>
		<field name="columa2" class="java.lang.String"/>
	</subDataset>
	<subDataset name="ReqMinimos">
		<parameter name="idConcursoPuestoAgr" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Long(15)]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT distinct
    rmc.orden as row_number,
    rmc.descripcion as columna1,
    dmr.minimos_requeridos as columna2

FROM planificacion.det_minimos_requeridos dmr

join planificacion.det_req_min drm
on (dmr.id_det_req_min = drm.id_det_req_min)

join planificacion.requisito_minimo_cpt rmc
on (drm.id_requisito_minimo_cpt = rmc.id_requisito_minimo_cpt)

where rmc.activo = true
and dmr.activo = true
and drm.tipo = 'GRUPO'
and id_concurso_puesto_agr = $P{idConcursoPuestoAgr}

order by row_number, columna1, columna2]]>
		</queryString>
		<field name="row_number" class="java.lang.Integer"/>
		<field name="columna1" class="java.lang.String"/>
		<field name="columna2" class="java.lang.String"/>
	</subDataset>
	<subDataset name="ReqA1">
		<parameter name="idConcursoPuestoAgr" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Long(15)]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT distinct
    rmc.orden  as row_number,
    rmc.descripcion as columna1,
    doc.opc_convenientes as columna2

FROM planificacion.det_opciones_convenientes doc

join planificacion.det_req_min drm
on (doc.id_det_req_min = drm.id_det_req_min)

join planificacion.requisito_minimo_cpt rmc
on (drm.id_requisito_minimo_cpt = rmc.id_requisito_minimo_cpt)

where rmc.activo = true
and doc.activo = true
and drm.activo = true
and drm.tipo = 'GRUPO'
and id_concurso_puesto_agr = $P{idConcursoPuestoAgr}

order by row_number, columna1, columna2]]>
		</queryString>
		<field name="row_number" class="java.lang.Integer"/>
		<field name="columna1" class="java.lang.String"/>
		<field name="columna2" class="java.lang.String"/>
	</subDataset>
	<subDataset name="condicion_trabajo">
		<parameter name="idCPAgr" class="java.lang.Long" isForPrompting="false">
			<defaultValueExpression><![CDATA[new Long(23)]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT distinct
    ct.descripcion as condicion_trabajo,
    puntaje_cond_trab,
    ct.orden

FROM planificacion.det_condicion_trabajo dct

JOIN planificacion.condicion_trabajo ct
on (dct.id_condicion_trabajo = ct.id_condicion_trabajo)

where dct.activo is true
    and ct.activo is true
    and tipo = 'GRUPO'
    and id_concurso_puesto_agr = $P{idCPAgr}

order by ct.orden]]>
		</queryString>
		<field name="condicion_trabajo" class="java.lang.String"/>
		<field name="puntaje_cond_trab" class="java.lang.Float"/>
		<field name="orden" class="java.lang.Integer"/>
	</subDataset>
	<subDataset name="condicion_especifica">
		<parameter name="idCPAgr" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT distinct
    cte.descripcion as condicion_trabajo_especif,
    dcte.puntaje_cond_trab_especif,
    cte.orden,
    dcte.justificacion,
    cte.tipo

FROM planificacion.det_condicion_trabajo_especif dcte

JOIN planificacion.condicion_trabajo_especif cte
on (dcte.id_condiciones_trabajo_especif = cte.id_condiciones_trabajo_especif)

where dcte.activo is true
    and cte.activo is true
    and dcte.tipo = 'GRUPO'
    and id_concurso_puesto_agr = $P{idCPAgr}

order by cte.orden]]>
		</queryString>
		<field name="condicion_trabajo_especif" class="java.lang.String"/>
		<field name="puntaje_cond_trab_especif" class="java.lang.Float"/>
		<field name="orden" class="java.lang.Integer"/>
		<field name="justificacion" class="java.lang.String"/>
		<field name="tipo" class="java.lang.String"/>
	</subDataset>
	<subDataset name="condicion_seguridad">
		<parameter name="idCPAgr" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT distinct
    cs.descripcion as condicion_seguridad,
    dcs.puntaje_cond_segur,
    dcs.justificacion,
    cs.orden

FROM planificacion.det_condicion_segur dcs

JOIN planificacion.condicion_segur cs
on (dcs.id_condicion_segur = cs.id_condicion_segur)

where dcs.activo is true
    and cs.activo is true
    and tipo = 'GRUPO'
    and id_concurso_puesto_agr = $P{idCPAgr}

order by cs.orden]]>
		</queryString>
		<field name="condicion_seguridad" class="java.lang.String"/>
		<field name="puntaje_cond_segur" class="java.lang.Float"/>
		<field name="justificacion" class="java.lang.String"/>
		<field name="orden" class="java.lang.Integer"/>
	</subDataset>
	<subDataset name="escala_trabajo">
		<queryString>
			<![CDATA[SELECT
  condicion_trabajo.descripcion cond,
  escala_cond_trab.descripcion escala,
  escala_cond_trab.desde,
  escala_cond_trab.hasta
FROM
  planificacion.condicion_trabajo,
  planificacion.escala_cond_trab
WHERE
  condicion_trabajo.id_condicion_trabajo = escala_cond_trab.id_condicion_trabajo AND
  condicion_trabajo.activo = 'TRUE' AND
  escala_cond_trab.activo = 'TRUE'
ORDER BY
  condicion_trabajo.orden ASC;]]>
		</queryString>
		<field name="cond" class="java.lang.String"/>
		<field name="escala" class="java.lang.String"/>
		<field name="desde" class="java.lang.Integer"/>
		<field name="hasta" class="java.lang.Integer"/>
	</subDataset>
	<subDataset name="escala_especificas">
		<queryString>
			<![CDATA[SELECT
  escala_cond_trab_especif.descripcion,
  escala_cond_trab_especif.desde,
  escala_cond_trab_especif.hasta
FROM
  planificacion.escala_cond_trab_especif
WHERE
  escala_cond_trab_especif.activo = 'TRUE'
ORDER BY
  escala_cond_trab_especif.desde ASC;]]>
		</queryString>
		<field name="descripcion" class="java.lang.String"/>
		<field name="desde" class="java.lang.Integer"/>
		<field name="hasta" class="java.lang.Integer"/>
	</subDataset>

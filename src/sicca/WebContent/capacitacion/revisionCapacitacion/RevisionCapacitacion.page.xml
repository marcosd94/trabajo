<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	no-conversation-view-id="/home.xhtml" login-required="true">

	<begin-conversation join="true" flush-mode="MANUAL" />

	<action execute="#{revisionCapacitacionFC.iniVer()}" on-postback="false" />
	<param name="capacitacionesIdCapacitacion" value="#{revisionCapacitacionFC.idCapacitacion}" />

	<param name="revisionCapacitacionFrom" />

	<param name="capacitacionesFrom" />
	<param name="from" />




	<navigation from-action="#{revisionCapacitacionFC.enviarAprobacion()}">
		<rule if-outcome="ok">
			<end-conversation before-redirect="true" />
			<redirect
				view-id="/capacitacion/bandeja/gestionarBandeja/BandejaCapacitacionList.xhtml">
				<param name="mensaje" value="ok" />
			</redirect>
		</rule>
	</navigation>


	<restrict>#{identity.hasRole('revisionCapacitacion_view')}</restrict>
</page>
